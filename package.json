    "@iconify-json/fluent": "1.0.15",
    "@sentry/tracing": "^6.19.7",
    "@sentry/vue": "^6.19.7",
    "@types/nprogress": "0.2.0",
    "@types/qrcode": "1.4.2",
    "@types/qs": "6.9.7",
