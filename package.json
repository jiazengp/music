    "@vue/runtime-dom": "3.2.33",
    "@vueuse/core": "8.5.0",
    "NeteaseCloudMusicApi": "^4.5.14",
    "axios": "0.26.1",
    "change-case": "4.1.2",
    "cli-color": "2.0.2",
    "color": "4.2.1",
    "conventional-changelog-cli": "2.2.2",
    "core-js": "3.20.3",
    "critters": "0.0.16",
    "crypto-js": "4.1.1",
    "cz-conventional-changelog": "3.3.0",
    "dayjs": "1.10.7",
